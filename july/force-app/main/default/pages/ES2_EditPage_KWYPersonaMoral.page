         <script>	           
    		function remplazar(valor){
                document.getElementById(valor).value = document.getElementById(valor).value.replace(/[^0-9]/,'');
                if(document.getElementById(valor).value.length > 2){
                	document.getElementById(valor).value = document.getElementById(valor).value.substring(0,2);
                }  
			}
        
        	function remplazarCp(valor){
                document.getElementById(valor).value = document.getElementById(valor).value.replace(/[^0-9]/,'');
                if(document.getElementById(valor).value.length > 5){
                	document.getElementById(valor).value = document.getElementById(valor).value.substring(0,5);
                }  
			}
        </script>
        <apex:pageBlock title="Customer Prospect Portal (Company)" id="Company">
