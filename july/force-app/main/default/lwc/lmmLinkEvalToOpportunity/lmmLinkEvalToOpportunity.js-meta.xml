            <property name="evalTypeSecondOpt" type="String" required="false" datasource="No Aplica,Proposal,Booking"/>
            <property name="evalTypeSecondOptStatus" type="String" required="false" datasource="ALL,NEW,REJECTED,PENDING,REJECTED,APPROVED,CANCELLED BY REQUESTOR" default="No Aplica"/>
        </targetConfig>
    </targetConfigs>
</LightningComponentBundle>
