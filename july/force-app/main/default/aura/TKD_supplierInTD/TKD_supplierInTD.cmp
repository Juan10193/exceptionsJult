                                                            <div class="slds-size_1-of-3" ><lightning:input name="vendorName"  aura:Id="vendorName" label="Name:" value="{! infoDetKnownSupp.vendorName}" maxlength="100"  variant="label-inline"/></div>
                                                            <div class="slds-size_1-of-3" ><lightning:input name="vendorSiteCodeAlt" aura:Id="vendorSiteCodeAlt"  label="Vendor Site Code: " value="{! infoDetKnownSupp.vendorNameAlt}"  variant="label-inline"/></div>                                           
                                                            <div class="slds-size_1-of-3" ><lightning:input name="vatRegistrationNum" aura:Id="vatRegistrationNum"  label="RFC:" value="{! infoDetKnownSupp.vatRegistrationNum}"  variant="label-inline"/></div>                                           
                                                      </div>                                   
                                                 </div>                                
                                              </article>
                                           </div>                       
                                        </article>
                                       </div>
                                         
                                         
                                       <div id="infoSite">
