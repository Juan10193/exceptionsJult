                                                                    <div class="slds-size_1-of-3" ><lightning:input name="" aura:id="" label="Phone" value="{! site.phone}" variant="label-inline"/></div>
                                                                    <div class="slds-size_1-of-3" ><lightning:input name="" aura:id="" label="Country" value="{! site.country}" variant="label-inline"/></div>
