  <aura:attribute name="supplierSelected" type="Object" />
  <aura:attribute name="siteSelected" type="Object" />
  <aura:attribute name="accountSelected" type="Object" />
  <!-- ./ atributos para  modal invoice -->
  <aura:handler name="init" value="{! this }" action="{! c.init }" />
  <!-- atributos par amodal Suppliers Documents-->
  <aura:attribute name="isModalDocsOpen" type="boolean" default="false"/>
  <aura:attribute name="filterKnownSuppliers" type="List" default="[]" />
  <aura:attribute name="sitesSelected" type="Supplier__c[]" default="[]" />
  <aura:attribute name="suppliersSelected" type="Supplier__c[]" />
  <!--Artributos para evento de detail site-->
  

  <c:TKD_siteDetail></c:TKD_siteDetail>
  <aura:if isTrue="{!v.isModalDocsOpen}">
    <c:takedownDocsSupplier recordId="{! v.recordId}" docshowModal="{!v.isModalDocsOpen}" onclosedocsmodal="{!c.isModalDocsClosed}"></c:takedownDocsSupplier>
  </aura:if>
  
   <!--Panel Status -->
            <article class="slds-card">
            <div class="slds-card__body slds-card__body_inner">
                <article class="slds-card slds-card_boundary">
                    <div class="slds-card__header slds-grid">
                        <header class="slds-media slds-media_center slds-has-flexi-truncate">
                            <div class="slds-media__figure"></div>
                            <div class="slds-media__body"><span></span>
                                <strong>AML Status : </strong><lightning:badge label="{! v.statusSuppApp.tkd_ls_Stage_AML__c}" style="background-color: #4DC3FF;"/> <strong style="margin-left:30px"> PROVEEDOR Status : </strong><lightning:badge label="{! v.statusSuppApp.tkd_ls_Stage_Supplier__c}" style="background-color: #4DC3FF;"/></div>
                                <lightning:buttonIcon iconName="utility:open_folder"  alternativeText="Suppliers Documents"
                                onclick="{!c.openDocsSuppliers}" />
                            <div class="slds-no-flex"><button class="slds-button slds-button_neutral" onclick="{! c.gotoURlSupplier}">Supplier in Approval</button></div>
                        </header>
                    </div>                   
                </article>
            </div>
        </article>
   <!--Panel Status -->
    
  <!--tableAmlHits -->
  <c:TDK_supplierInTD_tableAmlHits recordId="{! v.recordId}"></c:TDK_supplierInTD_tableAmlHits>
    
  <!--./tableAmlHits -->
      <div class="slds-no-flex"><lightning:button variant="brand" label="Add Supplier" iconName="action:new"  iconPosition="left" title="Add Supplier" onclick="{! c.openModel }"/></div>    
  <!-- 3ra Seccion -->
     <!--Tabs  -->  
    		<div id="tabsKNS" style="display:none">
              <!--Tabs New Supplier and Known Suplier iconName="action:new_group"-->
              <lightning:tabset variant="scoped">
                <lightning:tab label="Known Supplier" >
                    <aura:renderIf isTrue="{!v.visibilityDivSearch}">
                    	<div id="searchSuplier">
                         <article class="slds-card">                       
                           <div class="slds-card__body slds-card__body_inner">
                              <article class="slds-card slds-card_boundary">
                                 <div class="slds-card__header slds-grid">
                                    <header class="slds-media slds-media_center slds-has-flexi-truncate">
                                       <div class="slds-media__figure">  <lightning:icon  iconName="standard:search" size="small" variant="bare"/></div>
                                       <div class="slds-media__body">
                                          <h2 class="slds-card__header-title">
                                             <strong>Search Supplier</strong>
                                          </h2>
                                       </div>
                                    </header>
                                 </div>
                                 <div class="slds-card__body slds-card__body_inner">
                                     <div class="demo-only demo-only--sizing slds-grid slds-wrap" >
