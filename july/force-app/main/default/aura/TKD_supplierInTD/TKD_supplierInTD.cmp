                                                    <div class="slds-size_2-of-4" ><lightning:input name="SearchByNameS" aura:Id="fields"  label="Search by Name:" value="{! v.SearchByName}"  maxlength="500"  required="false" type="search" variant="label-inline"/></div>
                                                    <div class="slds-size_2-of-4" ><lightning:input name="SearchByCurrencyS" aura:Id="SearchByCurrencyS"  label="Search by Currency:" value="{! v.SearchByCurrency}"  maxlength="500" type="search"  variant="label-inline"/></div>
                                                    <div class="slds-size_2-of-4" ><lightning:input name="SearchByTaxCodeS" aura:Id="SearchByTaxCodeS"   label="Search by Tax Code:"  value="{! v.SearchByTaxCode}" maxlength="500" type="search"  variant="label-inline" /></div>
                                                  </div>
                                 </div>
                                 <footer class="slds-card__footer">
                                     <div class="slds-media__body"></div>
                                     <div class="slds-no-flex" ><lightning:button variant="brand" class="btn bg-gradient-cyan float-right" label="Search" iconName="utility:search" iconPosition="left" title="Search Suppliers" onclick="{! c.searchSuppliers }"/></div>
    
                                 </footer>
                              </article>
                           </div>                       
                    	</article>
                    </div>
                    </aura:renderIf>
                    <aura:renderIf isTrue="{!v.visibilityDivResult}">
