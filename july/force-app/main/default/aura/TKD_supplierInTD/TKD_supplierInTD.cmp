                                                                                    <header class="slds-media slds-media_center slds-has-flexi-truncate">
                                                                                       <div class="slds-media__figure"><lightning:icon  iconName="standard:account" size="small" variant="bare"/></div>
                                                                                       <div class="slds-media__body">
                                                                                          <h2 class="slds-card__header-title">
                                                                                             <strong>Information Account</strong>
                                                                                          </h2>
                                                                                       </div>
                                                                                       <div class="slds-no-flex"><lightning:button label="New" name="{!indexSupp+'-'+ indexSite}" iconName="utility:add" iconPosition="left" onclick="{! c.newAccount }"/></div>
                                                                                    </header>
                                                                                 </div>
                                                                                 <div class="slds-card__body slds-card__body_inner">
                                                                                     <table class="slds-table slds-table_cell-buffer slds-table_bordered slds-table_striped">
                                                                                        <thead class="gris text-center">
                                                                                          <tr class="slds-line-height_reset">
                                                                                            <th class="" scope="col"> <div class="slds-truncate" title="Originator" >Bank Name</div></th>
                                                                                            <th class="" scope="col"> <div class="slds-truncate" title="ISC">Bank Account Name</div> </th>
                                                                                            <th class="" scope="col"><div class="slds-truncate" title="Suppliers">Bank Account Num</div ></th>
                                                                                            <th class="" scope="col"> <div class="slds-truncate" title="AML Status">Currency</div > </th>
                                                                                            <th class="" scope="col"> <div class="slds-truncate" title="AML Status">Action</div > </th>
                                                                                          </tr>
                                                                                        </thead>
                                                                                        <tbody class="text-center">
                                                                                            <aura:iteration items="{! site.accounts.account}" var="account" indexVar="indexAcc">
                                                                                              <tr class="slds-hint-parent"> 
                                                                                                <td ><div class="slds-truncate">{! account.bankName}</div >
                                                                         
                                                                                                    
                                                                                                  </td>
                                                                                                <td> <div class="slds-truncate" >{! account.bankAccountName}</div ></td>
                                                                                                <td class="text-center" >{! account.bankAccountNum}</td>
                                                                                                <td class="text-center" >{! account.curency}</td >
                                                                                                <td class="text-center"> <lightning:button aura:id="{!indexSupp}" name="{!indexSupp+'-'+ indexSite +'-'+indexAcc}" iconName="utility:edit" iconPosition="left" tabindex="{!indexAcc}"  variant="brand" onclick="{! c.showDetailsAccount }" /></td>
                                                                                              </tr>
                                                                                            </aura:iteration>                                                
                                                                                        </tbody>
                                                                                      </table>                                   
                                                                                 </div>                                
                                                                              </article>
                                                                           </div>                       
                                                                        </article>
                                                                </div><br></br>
