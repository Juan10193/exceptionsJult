                                                                                 <div class="slds-card__header slds-grid">
                                                                                    <header class="slds-media slds-media_center slds-has-flexi-truncate">
                                                                                       <div class="slds-media__figure"><lightning:icon  iconName="standard:contact" size="small" variant="bare"/></div>
                                                                                       <div class="slds-media__body">
                                                                                          <h2 class="slds-card__header-title">
                                                                                             <strong>Information Contact</strong>
                                                                                          </h2>
                                                                                       </div>
                                                                                       <div class="slds-no-flex"><lightning:button label="New" iconName="utility:add" iconPosition="left" onclick="{! c.newContact }"/></div>
                                                                                    </header>
                                                                                 </div>
                                                                                 <div class="slds-card__body slds-card__body_inner">
                                                                                     <table class="slds-table slds-table_cell-buffer slds-table_bordered slds-table_striped">
                                                                                        <thead class="gris text-center">
                                                                                          <tr class="slds-line-height_reset">
                                                                                            <th class="" scope="col"> <div class="slds-truncate" title="Originator" >First Name</div></th>
                                                                                            <th class="" scope="col"> <div class="slds-truncate" title="ISC">Last Name</div> </th>
                                                                                            <th class="" scope="col"> <div class="slds-truncate" title="ISC">Phone</div> </th>
                                                                                            <th class="" scope="col"><div class="slds-truncate" title="Suppliers">Email Address</div ></th>
                                                                                            <th class="" scope="col"><div class="slds-truncate" title="Suppliers">Action</div ></th>
                                                                                          </tr>
                                                                                        </thead>
                                                                                        <tbody class="text-center">
                                                                                           <aura:iteration items="{! site.contacts.contact}" var="contact" indexVar="indexCon">
                                                                                              <tr class="slds-hint-parent"> 
                                                                                                <td ><div class="slds-truncate">{! contact.personFirstName}</div ></td>
                                                                                                <td> <div class="slds-truncate" >{! contact.personLastName}</div ></td>
                                                                                                <td> <div class="slds-truncate" >{! contact.phone}</div ></td>
                                                                                                <td class="text-center" >{! contact.emailAddress}</td>
                                                                                                <td class="text-center"><lightning:button name="{!indexSupp+'-'+ indexSite +'-'+indexCon}"  iconName="utility:edit" iconPosition="left" tabindex="{!indexCon}"  variant="brand" onclick="{! c.showDetailsContacts }" /> </td> 
                                                                                              </tr>
                                                                                            </aura:iteration>                                                
                                                                                        </tbody>
                                                                                      </table>                                                                                     
                                                                                 </div>                                
                                                                              </article>
                                                                           </div>                       
                                                                        </article>
                                                                </div>
                                                                 
                                                            </lightning:accordionSection>
                                                        </lightning:accordion>                                               
                                                    </aura:iteration>                                        
                                                 </div>                                
                                              </article>
                                           </div>                       
                                        </article>
                                       </div>
                                                   
                                    </aura:iteration>
                                 </div> 
                                 <footer class="slds-media slds-media_center slds-has-flexi-truncate">
                                  <div class="slds-media__body"></div>
                                  <div class="slds-no-flex">
                                      <lightning:button variant="brand" label="Save" iconName="utility:save"  iconPosition="left" title="Save" onclick="{! c.onSave }"/>
                                      <lightning:button  label="Cancel" iconName="utility:close"  iconPosition="left" title="Add Supplier" onclick="{! c.cancelDetails }"/>
                                  </div>
                                </footer>
                              </article>
                           </div>                       
                    	</article>
                    </div>
                    </aura:renderIf>
                    
                    <br></br><br></br>
                    <aura:renderIf isTrue="{!v.visibilityDivSearch == true &amp;&amp; v.visibilityDivResult == true}">
                    	<footer class="slds-media slds-media_center slds-has-flexi-truncate">
                                  <div class="slds-media__body"></div>
                                  <div class="slds-no-flex">
                                      <lightning:button  label="Cancel" iconName="utility:close"  iconPosition="left" title="Add Supplier" onclick="{! c.cancelSearch }"/>
                                  </div>
                    </footer>
                    </aura:renderIf>

                  <!--./ Informacion de la tab Known Supplier iconName="action:join_group" -->
