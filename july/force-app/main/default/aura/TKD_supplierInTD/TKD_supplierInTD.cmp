<aura:component controller="Takedown_Controller_SupplierInCA" implements="lightning:actionOverride,force:hasRecordId,force:hasSObjectName,flexipage:availableForAllPageTypes" access="global">
<aura:registerEvent name="detailsitesEvent" type="c:detailsitesEvent"/>
<aura:attribute name="isOpen" type="boolean" default="false" />
