                                                                    <div class="slds-size_1-of-3" ><lightning:input name="" aura:id="" label="City" value="{! site.city}" variant="label-inline"/></div>
                                                                    <div class="slds-size_1-of-3" ><lightning:input name="" aura:id="" label="State" value="{! site.state}" variant="label-inline"/></div>
                                                                    <div class="slds-size_1-of-3" ><lightning:input name="" aura:id="" label="ZIP" value="{! site.zip}" variant="label-inline"/></div>
